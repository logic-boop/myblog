{
  "functions": {
    "api/**/*.php": {
      "runtime": "vercel-php@0.7.4"
    },
    "includes/**/*.php": {
      "runtime": "vercel-php@0.7.4"
    },
    "*.php": {
      "runtime": "vercel-php@0.7.4"
    }
  },
  "routes": [
    {
      "src": "/update.php",
      "dest": "/includes/update.php"
    },
    {
      "src": "/write_intel_99.php",
      "dest": "/includes/write_intel_99.php"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}